ct() {
  name="main"
  mount_home=no
  gpu=no
  image="docker.io/archlinux/archlinux:latest"

  for arg do
    [ "$arg" = "--help" ] && { printf 'Usage: %s [-n NAME] [-h]\n' "ct" >&2; return 0; }
  done

  OPTIND=1
  while getopts "n:i:hg" opt; do
    case "$opt" in
      n) name=$OPTARG ;;
      i) image=$OPTARG ;;
      h) mount_home=yes ;;
      g) gpu=yes ;;
      \?|:) printf 'Usage: %s [-n NAME] [-i IMAGE] [-h]\n  -n NAME    Container name suffix (creates ct-NAME, default: ct)\n  -h         Enable home directory mounting\n  -g         Enable gpu\n' "ct" >&2; return 1 ;;
    esac
  done
  shift $((OPTIND - 1))
  container_name="ct-$name"

  # WAYLAND volume is for clipboard
  gui_args="-v /run/user/$(id -u):/run/user/$(id -u) \
      -e WAYLAND_DISPLAY \
      -e XDG_RUNTIME_DIR"

  # mount home dir
  mount_args=""
  if [ "$mount_home" = "yes" ]; then
    mount_args="-v $HOME:/root/user_home:rslave"
  fi

  # gpu
  dri=""
  if [ "$gpu" = "yes" ]; then
    dri="--device=/dev/dri:/dev/dri"
  fi

  # shm-size is needed to prevent chromium apps from crashing inside the container since podmans default is 64m
  podman container exists "$container_name" 2>/dev/null || podman container create -it \
    --workdir /root \
    --shm-size=1g \
    $gui_args \
    $dri \
    --security-opt label=disable $mount_args \
    --name "$container_name" \
    "$image"
    # sh -c 'exec /bin/sh'

  if ! podman inspect "$container_name" --format='{{.State.Running}}' 2>/dev/null | grep -q true; then
    podman start "$container_name"
    podman exec -it "$container_name" /bin/bash
  else
    podman exec -it "$container_name" /bin/bash
  fi
}
ct-setup() {
  if command -v pacman > /dev/null 2>&1; then
    pacman -Sy --needed archlinux-keyring --noconfirm && pacman -Syyuu gcc openssh git curl unzip wl-clipboard nvim mergiraf chromium --noconfirm
  fi

  mkdir -p $HOME/.config && ln -s $PWD/user_home/.config/nvim $HOME/.config/
  mkdir -p $HOME/.config && ln -s $PWD/user_home/.config/git $HOME/.config/
  mkdir -p $HOME/.config && ln -s $PWD/user_home/.config/.shrc $HOME/.config/.shrc
  echo "source $HOME/.config/.shrc" >> $HOME/.bashrc
  ln -s $PWD/user_home/Documents/.claude $HOME/
  ln -s $PWD/user_home/Documents/.codex $HOME/
  rm -rf $HOME/.ssh && ln -s $PWD/user_home/.ssh $HOME/
}

vm() {
  os="linux"
  name="main"
  dist="arch" # tails, fedora, ubuntu, ...

  for arg do
    [ "$arg" = "--help" ] && { printf 'Usage: %s [-o linux/windows] [-n NAME] [-d DIST]\n' "vm" >&2; return 0; }  # use "exit 0" if not in a function
  done

  OPTIND=1
  while getopts "o:n:d:h" opt; do
    case "$opt" in
      o) os=$OPTARG ;;
      n) name=$OPTARG ;;
      d) dist=$OPTARG ;;
      h) printf 'Usage: %s [-o linux/windows] [-n NAME] [-d DIST]\n' "vm" >&2; return 0 ;; # or: exit 0
      \?|:) usage; return 1 ;; # or: exit 1
    esac
  done
  shift $((OPTIND - 1))
  name="vm-$name"

  is_port_free=no
  podman ps -a --format '{{.Ports}}' \
  | tr ', ' '\n' \
  | awk 'match($0,/([0-9]+)->/,m){print m[1]}' \
  | grep -qx 40000 && is_port_free=no || is_port_free=yes
  port="40000:8006"
  if [ "$is_port_free" = no ]; then
    port="127.0.0.1::8006"
  fi

  is_ssh_port_free=no
  podman ps -a --format '{{.Ports}}' \
  | tr ', ' '\n' \
  | awk 'match($0,/([0-9]+)->/,m){print m[1]}' \
  | grep -qx 2222 && is_ssh_port_free=no || is_ssh_port_free=yes
  ssh_port="2222:22"
  if [ "$is_ssh_port_free" = no ]; then
    ssh_port="127.0.0.1::22"
  fi

  mkdir -p "$HOME"/Desktop/shared
  if [ "$os" = "linux" ]; then
    podman run -d -p "$port" -p "$ssh_port" \
    --device=/dev/kvm \
    --device=/dev/net/tun \
    --cap-add NET_ADMIN \
    -e RAM_SIZE="4G" \
    -e CPU_CORES="6" \
    -e DISK_SIZE="64G" \
    -e BOOT_MODE="legacy" \
    -e DISK_TYPE="ide" \
    -e BOOT="$dist" \
    -v "$HOME"/Desktop/shared:/shared:z \
    --name "$name" \
    docker.io/qemux/qemu
  fi
  if [ "$os" = "windows" ]; then
    # shared dir on windows is at "\\20.20.20.1" or "\\host.lan\Data"
    # sunshine ports: 47984, 47989, 48010
    # 40001 configured for custom rdp
    podman run -d -p "$port" -p 40001:3389 \
    --device=/dev/kvm \
    --device=/dev/net/tun \
    --cap-add NET_ADMIN \
    -e RAM_SIZE="10G" \
    -e CPU_CORES="8" \
    -e DISK_SIZE="64G" \
    -v "$HOME"/Desktop/shared:/data:z \
    --name "$name" \
    docker.io/dockurr/windows:latest
  fi

}

